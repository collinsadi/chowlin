<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Vendor Password Reset</title>
    <%- include("../partials/universal/styles.ejs") %>
    <link rel="stylesheet" href="/styles/vendor/login.css">
    <link rel="stylesheet" href="/styles/alert/alert.css">


</head>
<body>
    

    <div class="page-container">


        <div class="login-left">

            <div class="login-left-inner">

            <div class="login-title">

            <h3>Welcome Back Vendor </h3>

            </div>


            <div id="" class="login-form">

                <p>Choose a New Password</p>


                <div class="form-group">

                    <!-- <p>Email</p> -->

                    <div class="form-cont">

                        <div class="input-icon">
                        <i class="fa-solid fa-lock"></i>
                        </div>
                        <input id="password" autocomplete="off" placeholder="New Password" name="password" type="password">
                    </div>

                </div>
                <div class="form-group">

                    <!-- <p>Email</p> -->

                    <div class="form-cont">

                        <div class="input-icon">
                        <i class="fa-solid fa-lock"></i>
                        </div>
                        <input id="confirmpassword" autocomplete="off" placeholder="Confirm New Password" name="Password" type="password">
                    </div>

                </div>

              

                <div class="form-group">
                    <button id="reset_password">Reset Password</button>
                </div>

             



            </div>
         






            </div>



        </div>










        <div class="login-right"> </div>

    <%- include("../partials/alert/alert.ejs") %>


    </div>


    <%- include("../partials/universal/scripts.ejs") %>
    <script src="/scripts/alert/alert.js"></script>
    <script src="/scripts/vendor/passwordReset.js"></script>

    
</body>
</html>